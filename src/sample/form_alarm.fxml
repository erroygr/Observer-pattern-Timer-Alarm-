<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="root" prefHeight="416.0" prefWidth="635.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ListView fx:id="list_view" layoutX="30.0" layoutY="128.0" prefHeight="236.0" prefWidth="541.0" />
      <Label layoutX="14.0" layoutY="14.0" prefHeight="42.0" prefWidth="460.0" text="Введите время (в сек.) на сколько поставить будильник:">
         <font>
            <Font name="Times New Roman" size="18.0" />
         </font>
      </Label>
      <Button layoutX="30.0" layoutY="67.0" mnemonicParsing="false" onAction="#addAlarm" prefHeight="42.0" prefWidth="127.0" text="Завести">
         <font>
            <Font name="Times New Roman" size="24.0" />
         </font>
      </Button>
      <TextField fx:id="values" layoutX="455.0" layoutY="13.0" prefHeight="44.0" prefWidth="115.0" promptText="в секундах">
         <font>
            <Font name="Times New Roman" size="18.0" />
         </font>
      </TextField>
      <Button layoutX="386.0" layoutY="67.0" mnemonicParsing="false" onAction="#deleteAlarm" prefHeight="44.0" prefWidth="235.0" text="Удалить будильник">
         <font>
            <Font name="Times New Roman" size="24.0" />
         </font>
      </Button>
      <Button layoutX="181.0" layoutY="66.0" mnemonicParsing="false" onAction="#ColorChange" prefHeight="45.0" prefWidth="188.0" text="Изменить цвет">
         <font>
            <Font name="Times New Roman" size="24.0" />
         </font>
      </Button>
      <Label fx:id="arr" layoutX="53.0" layoutY="364.0" prefHeight="42.0" prefWidth="460.0">
         <font>
            <Font name="Times New Roman" size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
